pipeline {
  agent any
  triggers {
    githubPush()
  }
  stages {
    stage('WWW') {
      steps {
        sh '''
         for f in $(git diff --name-only HEAD HEAD~1 | grep Website | awk -F'/' '{print $2}' | sort -u); do
          git checkout main
          git pull --all -f
          cp -rf Website/$f /var/www_data/$folder
         done
        '''
      }
    }
  }
}
