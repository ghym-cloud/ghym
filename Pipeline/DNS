pipeline {
  agent any
  triggers {
    githubPush()
  }
  stages {
    stage('DNS') {
      steps {
        sh '''
         a='          value: "'
         ptr='          value: "'
         while read -r line
         do
          a="${a}$line,"
          ptr="${a}$(echo $line | sed -e 's/(.*)=(.*)/\2=\1/')"
         done < DNS
         a="${a}\""
         ptr="${a}\""
         echo $a
        '''
      }
    }
  }
}
